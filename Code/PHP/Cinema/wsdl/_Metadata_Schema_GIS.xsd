<xs:schema elementFormDefault="qualified" targetNamespace="http://services.sapo.pt/Metadata/GIS" xmlns:tns="http://services.sapo.pt/Metadata/GIS" xmlns:xs="http://www.w3.org/2001/XMLSchema">
